# Google Sheets Integration (Set these in Netlify Environment Variables)
VITE_GOOGLE_SHEETS_API_KEY=your_google_api_key_here
VITE_GOOGLE_SHEETS_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_SHEETS_CLIENT_SECRET=your_google_client_secret_here
VITE_GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id_here

# Application Configuration
VITE_APP_VERSION=1.0.0-beta
VITE_LOG_LEVEL=info
VITE_ENABLE_AUTO_BACKUP=true
VITE_ENABLE_GOOGLE_SHEETS=true
VITE_ENABLE_ERROR_REPORTING=false