# Google Sheets Integration - Service Account (Set these in Netlify Environment Variables)
VITE_GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id_here
VITE_GOOGLE_SHEETS_SERVICE_EMAIL=your_service_account_email_here
VITE_GOOGLE_SHEETS_SERVICE_KEY=your_service_account_private_key_here

# Application Configuration
VITE_APP_VERSION=1.0.0-beta
VITE_LOG_LEVEL=info
VITE_ENABLE_AUTO_BACKUP=true
VITE_ENABLE_GOOGLE_SHEETS=true
VITE_ENABLE_ERROR_REPORTING=false